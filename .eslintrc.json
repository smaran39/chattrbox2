{
    "env": {
        "browser": true
    },
    "extends": "eslint:recommended",
    "rules": {
      "no-console":0,
      "no-undef":0,
      "no-unused-vars":0,
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}
